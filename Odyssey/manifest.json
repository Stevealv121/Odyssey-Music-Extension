{
    "manifest_version": 2,
    "name": "Odyssey Music Player",
    "version": "0.1",
    "description": "Music player extension for youtube",
    "icons": {
        "128": "icons/icon128.png",
        "48": "icons/icon48.png",
        "16": "icons/icon16.png"
    },
    "browser_action":{
        "default_icon": "icons/icon16.png",
        "default_popup": "popup.html"
    },
    "omnibox": {"keyword" : "@Odyssey"}
    
}